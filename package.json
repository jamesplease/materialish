{
  "name": "materialish",
  "version": "0.17.0-beta.3",
  "description": "React components that loosely follow Material Design",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint components stories",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run clean",
    "storybook": "npm run icons && start-storybook -p 9001 -c .storybook",
    "clean": "rimraf ./dist ./es ./tmp ./lib ./*.js ./*.css ./material-icons-repo ./icons ./icons-src ./icons-index-src icons-data.json ./src/icons ./transpiled-icons",
    "copy-source": "node ./build/copy-source-files.js",
    "concat-css": "node ./build/concatenate-css-files.js",
    "icons": "rimraf ./material-icons-repo && npm run generate-icons && npm run transpile-icons && npm run transpile-icons-index && npm run move-icons",
    "generate-icons": "node ./build/generate-icons.js",
    "prettier": "prettier --config ./.prettierrc --write **/*.{js,css}",
    "prebuild": "npm run clean",
    "build": "npm run icons && npm run build:umd && npm run build:umd:min && npm run build:es && npm run build:commonjs && npm run copy-source && npm run concat-css",
    "transpile-icons": "cross-env BABEL_ENV=commonjs babel src/icons/*.js --out-dir ./transpiled-icons",
    "move-icons": "node ./build/copy-transpiled-icons.js",
    "transpile-icons-index": "cross-env BABEL_ENV=commonjs babel icons-index-src --out-dir ./icons",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env NODE_ENV=development BABEL_ENV=build rollup -c build/rollup.config.js -i src/index.js -o dist/materialish.js",
    "build:umd:min": "cross-env NODE_ENV=production BABEL_ENV=buildProd rollup -c build/rollup.config.js -i src/index.js -o dist/materialish.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jamesplease/materialish.git"
  },
  "keywords": [
    "react",
    "components",
    "material",
    "design",
    "ux",
    "ui"
  ],
  "author": "Javier Porrero <porrero.javier@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jamesplease/materialish/issues"
  },
  "files": [
    "dist",
    "lib",
    "es",
    "icons",
    "*.js",
    "*.css",
    "icons-data.json"
  ],
  "homepage": "https://materialish.js.org",
  "dependencies": {
    "classnames": "^2.2.5",
    "focus-visible": "4.1.5",
    "prop-types": "^15.6.1",
    "react": "^16.0.0",
    "uniqid": "5.0.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.15",
    "@storybook/addon-knobs": "^3.3.15",
    "@storybook/addon-links": "^3.3.15",
    "@storybook/addon-notes": "^3.3.15",
    "@storybook/addon-options": "^3.3.15",
    "@storybook/addons": "^3.3.15",
    "@storybook/react": "^3.3.14",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chalk": "^2.3.2",
    "clean-css": "^4.1.11",
    "cross-env": "^5.1.4",
    "eslint": "^4.19.0",
    "eslint-plugin-react": "^7.7.0",
    "find": "^0.2.9",
    "git-clone": "^0.1.0",
    "glob": "^7.1.2",
    "lodash": "^4.17.10",
    "mkdirp": "^0.5.1",
    "prettier": "^1.11.1",
    "react-dom": "^16.4.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0",
    "storybook-addon-jsx": "^5.3.0"
  }
}
